<Window x:Class="AssetBrowser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AssetBrowser"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <WebBrowser Name="Browser" HorizontalAlignment="Left" Height="373" VerticalAlignment="Top" Width="792" Margin="0,46,0,0"/>
        <TextBox Name="IdSearch" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" Text="id" VerticalAlignment="Top" Width="120"/>
        <Button Name="Go" Content="Go" HorizontalAlignment="Left" Margin="135,9,0,0" VerticalAlignment="Top" Width="49" Height="24" Click="Go_Click"/>
    </Grid>
</Window>
